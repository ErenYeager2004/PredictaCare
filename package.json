{
  "name": "predictacare",
  "version": "1.0.0",
  "description": "PredictaCare is a comprehensive healthcare platform...",
  "main": "index.js",
  "scripts": {
    "frontend": "cd frontend && npm run dev",
    "admin": "cd admin && npm run dev",
    "node-backend": "cd backend && node server.js",
    "python-ml": "cd backend && python server.py",
    "dev": "concurrently \"npm run node-backend\" \"npm run python-ml\"",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:admin": "cd admin && npm install && npm run build",
    "build:backend": "cd backend && npm install",
    "build:python": "cd backend && pip install -r requirements.txt",
    "build": "npm install && npm run build:frontend && npm run build:admin && npm run build:backend && npm run build:python"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deyasin-Srijon/PredictaCare.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Deyasin-Srijon/PredictaCare/issues"
  },
  "homepage": "https://github.com/Deyasin-Srijon/PredictaCare#readme",
  "dependencies": {
    "concurrently": "^9.1.2",
    "react-markdown": "^10.1.0",
    "serve": "^14.2.4"
  }
}
